{
  "name": "silhouette",
  "version": "0.32.0",
  "description": "Simple task management tools for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "bun esbuild.config.mts",
    "build": "tsgo -noEmit -skipLibCheck && bun esbuild.config.mts production",
    "check:format": "prettier -l \"**/*.ts\"",
    "pre:push": "tsgo -noEmit -skipLibCheck && bun run check:format && bun run test",
    "test": "bun test",
    "ci": "bun install --frozen-lockfile --ignore-scripts && bun run pre:push && bun run build",
    "release": "bun scripts/release.mts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@event-calendar/core": "2.7.2",
    "@event-calendar/day-grid": "2.7.2",
    "@tsconfig/svelte": "5.0.4",
    "@types/bun": "1.3.3",
    "@types/node": "22.16.5",
    "@typescript/native-preview": "7.0.0-dev.20251127.1",
    "builtin-modules": "5.0.0",
    "chokidar": "5.0.0",
    "esbuild": "0.27.0",
    "esbuild-svelte": "0.8.2",
    "obsidian": "1.10.3",
    "prettier": "3.7.1",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "4.2.18",
    "svelte-preprocess": "5.1.4",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@tadashi-aikawa/silhouette-core": "npm:@jsr/tadashi-aikawa__silhouette-core@1.7.0",
    "owlelia": "0.49.1",
    "ts-pattern": "5.9.0"
  }
}
