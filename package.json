{
  "name": "silhouette",
  "version": "0.16.0",
  "description": "Simple task management tools for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
